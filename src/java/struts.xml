<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        
        <action name="Login" class="paquete.LoginAction">
            <result name="exitoso">admin.jsp</result>
            <result name="exitosop">pacientei.jsp</result>
            <result name="exitosom">medicoi.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        
        <action name="Add" class="paquete.AddAction">
            <result name="exitoso">admin.jsp</result>
            <result name="error">admin.jsp</result>
        </action>
        
        <action name="MAdd" class="paquete.MAddAction">
            <result name="exitoso">medicos.jsp</result>
            <result name="error">medicos.jsp</result>
        </action>
        
        <action name="Del" class="paquete.DeleteAction">
            <result name="exitoso">admin.jsp</result>
            <result name="error">admin.jsp</result>
        </action>
        
        <action name="Mod" class="paquete.ModifyAction">
            <result name="exitoso">admin.jsp</result>
            <result name="error">admin.jsp</result>
        </action>
        
        <action name="MMod" class="paquete.MModifyAction">
            <result name="exitoso">medicos.jsp</result>
            <result name="error">medicos.jsp</result>
        </action>
        
        <action name="MDel" class="paquete.MDeleteAction">
            <result name="exitoso">medicos.jsp</result>
            <result name="error">medicos.jsp</result>
        </action>
        
        <action name="PAdd" class="paquete.PAddAction">
            <result name="exitoso">registroPaciente.jsp</result>
            <result name="error">pacientes.jsp</result>
        </action>
        
        <action name="PAddC" class="paquete.CaddAction">
            <result name="exitoso">pacientei.jsp</result>
            <result name="error">pacientei.jsp</result>
        </action>
        
        <action name="MAddCons" class="paquete.MaddConsulta">
            <result name="exitoso">medicoi.jsp</result>
            <result name="error">medicoi.jsp</result>
        </action>     
        
        <action name="ModifyPac" class="paquete.ModifyP">
            <result name="exitoso">pacientes.jsp</result>
            <result name="error">pacientes.jsp</result>
        </action>
        
        <action name="deletePAC" class="paquete.deletePaciente">
            <result name="exitoso">pacientes.jsp</result>
            <result name="error">pacientes.jsp</result>
        </action>
        
    </package>
</struts>
